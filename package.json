{
  "name": "ccsync",
  "version": "0.6.2",
  "description": "Fast rsync-powered two-way sync with SQLite metadata and SSH-friendly scanning.",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "bin": {
    "ccsync": "bin/ccsync.mjs"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist node_modules",
    "prepare": "pnpm test",
    "test": "pnpm build && NODE_OPTIONS='--no-warnings' jest",
    "test:watch": "pnpm build &&  NODE_OPTIONS='--no-warnings' jest --watch"
  },
  "engines": {
    "node": ">=24"
  },
  "files": ["dist", "README.md", "LICENSE"],
  "keywords": ["rsync", "sqlite", "sync", "watch", "ssh", "mutagen", "backup"],
  "license": "MIT",
  "packageManager": "pnpm@10.19.0",
  "dependencies": {
    "@node-rs/xxhash": "^1.7.6",
    "@nodelib/fs.walk": "^3.0.1",
    "ascii-table3": "^1.0.1",
    "chokidar": "^4.0.3",
    "commander": "^14.0.2",
    "ignore": "^7.0.5"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.9.1",
    "jest": "^29.7.0",
    "ts-jest": "29",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "author": "William Stein (wstein@sagemath.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/ccsync"
  },
  "homepage": "https://github.com/sagemathinc/ccsync"
}
